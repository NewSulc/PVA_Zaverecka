<Window x:Class="ZaverecnaPrace.DeleteUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZaverecnaPrace"
        mc:Ignorable="d"
        Title="DeleteUser" Height="500" Width="500" Background="#FF004225">
    <Grid>
        <ComboBox x:Name="DropdownDelete" HorizontalAlignment="Center" Margin="0,154,0,0" VerticalAlignment="Top" Width="300" Height="50" FontSize="20" Background="#FFF2F2F2"/>
        <Button x:Name="DeleteBtn" Content="Delete user?" HorizontalAlignment="Center" Margin="0,284,0,0" VerticalAlignment="Top" Width="300" Height="50" Background="#FFF2F2F2" Click="Delete"/>
    </Grid>
</Window>
